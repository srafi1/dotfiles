{
    "$schema": "http://json-schema.org/draft-07/schema",
    "description": "Configuration for coc.nvim",
    "languageserver": {
        "python": {
            "command": "python",
            "args": [
                "-mpyls"
            ],
            "filetypes": [
                "python"
            ]
        },
        "go": {
            "command": "gopls",
            "rootPatterns": ["go.mod"],
            "filetypes": [
                "go"
            ]
        },
        "java": {
            "command": "jdtls",
            "filetypes": [
                "java"
            ]
        },
        "clang": {
            "command": "clangd",
            "filetypes": [
                "c",
                "cpp",
                "cc"
            ]
        },
        "latex": {
            "command": "texlab",
            "filetypes": [
                "tex"
            ]
        }
    }
}
